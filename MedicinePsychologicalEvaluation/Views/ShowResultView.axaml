<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:vm="using:MedicinePsychologicalEvaluation.ViewModels"
             x:Class="MedicinePsychologicalEvaluation.Views.ShowResultView">
  <Design.DataContext>
    <vm:ShowResultViewModel/>
  </Design.DataContext>

  <StackPanel>
    <Button Command="{Binding GoBack}" Margin="10">返回</Button>
    <TextBlock Text="{Binding TestResult}" Margin="20" VerticalAlignment="Center" HorizontalAlignment="Center" />
  </StackPanel>
  
</UserControl>
